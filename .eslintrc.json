{
  "parser": "@typescript-eslint/parser",
  // "extends": "next/core-web-vitals",
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": 1,
    "no-explicit-any": "off",
    "no-case-declarations": "off",
    "no-undef": 2,
    "no-constant-condition": "off",
    "no-empty": "off"
  }
}
